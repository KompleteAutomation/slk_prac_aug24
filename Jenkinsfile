pipeline {
    agent any
	

    stages {
        stage('Clean') {
            steps {
                echo 'Clean'
                
            }   
        }
		
		stage('Build') {
            steps {
                echo 'Build'
		        
            }   
        }
		
		stage('Deploy') {
            steps {
                echo 'Deploy'
		        
            }   
        }
         
    }
}